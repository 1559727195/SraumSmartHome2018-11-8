<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ywl="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/dialog_corner_bg"
    android:orientation="vertical">

    <TextView
        android:id="@+id/promat_txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="提示"
        android:textColor="#000000"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/name_gloud"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="提示"
        android:textColor="#000000"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="30dp"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="确定要删除?"
        android:textColor="#000000"
        android:textSize="16sp"
        android:visibility="gone" />
    <!--<View-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="1dp"-->
    <!--android:background="@color/e0"></View>-->
    <com.massky.sraum.view.DownLoadProgressbar
        android:id="@+id/dp_game_progress"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y15"
        android:layout_margin="@dimen/x8"
        ywl:dptextsize="14sp"
        ywl:offsetRight="5dp"></com.massky.sraum.view.DownLoadProgressbar>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:padding="5dp">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/x8"
            android:layout_marginRight="@dimen/x8"
            android:layout_marginBottom="@dimen/y5"
            android:layout_weight="0.45"
            android:background="@drawable/dialog_text_corner_bg">

            <TextView
                android:id="@+id/call_cancel"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="取消"
                android:textColor="#000000"
                android:textSize="16dp" />
        </RelativeLayout>

        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:layout_weight="0.1" />

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="5dp"
            android:visibility="gone"
            android:layout_weight="0.45"
            android:background="@drawable/dialog_text_corner_bg">

            <TextView
                android:id="@+id/call_confirm"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="是"
                android:textColor="#000000"
                android:textSize="16dp" />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>