<?xml version="1.0" encoding="utf-8"?><!--关于界面item-->
<com.mcxtzhang.swipemenulib.SwipeMenuLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipe_menu_layout"
    android:layout_width="match_parent"
    android:layout_height="@dimen/y30"
    android:background="@color/white"
    app:ios="false"
    app:leftSwipe="true"
    app:swipeEnable="true"
    android:clickable="true">


    <RelativeLayout
        android:id="@+id/swipe_context"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/rel_scene_set"
                android:layout_width="match_parent"
                android:layout_height="60dp">

                <ImageView
                    android:id="@+id/iv_toaddedit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/wode_right_arrow" />

                <TextView
                    android:id="@+id/tv_timing_week"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp"
                    android:layout_toLeftOf="@+id/iv_toaddedit"
                    android:textColor="@color/color_alarm_textcolor"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tv_timing_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/tv_timing_week"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="19dp"
                    android:textColor="@color/color_alarm_textcolor"
                    android:textSize="16sp" />
            </RelativeLayout>

            <View
                android:id="@+id/three_s"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/dottedline" />
        </LinearLayout>
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/swipe_right_menu"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/discard"
                android:layout_width="60dp"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:background="#FB3C30" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="删除"
                android:textColor="#FFFFFF"
                android:textSize="15sp" />
        </RelativeLayout>
    </LinearLayout>
</com.mcxtzhang.swipemenulib.SwipeMenuLayout>

